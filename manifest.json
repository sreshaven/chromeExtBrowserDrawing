{
  "manifest_version": 2,
  "name": "Browser Drawing Extension",
  "version": "2.0",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["p5.js", "p5.dom.min.js", "content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "brush.png"
  }
}
